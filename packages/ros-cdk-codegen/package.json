{
  "name": "@alicloud/ros-cdk-codegen",
  "version": "1.0.8",
  "description": "",
  "files": [
    "lib/**/*",
    "bin/",
    "script/"
  ],
  "scripts": {
    "build": "tsc",
    "genspec": "tsc && node ./bin/spec2ts --spec=true",
    "spec2ts": "tsc && node ./bin/spec2ts && node ./script/integrate.js",
    "format": "prettier --write \"lib/**/*.{js,ts}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aliyun/Resource-Orchestration-Service-Cloud-Development-Kit.git"
  },
  "dependencies": {
    "@alicloud/ros-2019-09-10": "^1.0.0",
    "@alicloud/ros-cdk-spec": "^1.11.0",
    "codemaker": "^1.7.0",
    "fs-extra": "^9.0.1",
    "readline-sync": "^1.4.10",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/node": "~18.11.9",
    "@types/fs-extra": "^8.1.0",
    "@types/readline-sync": "^1.4.3",
    "@types/yargs": "^15.0.5",
    "ncp": "^2.0.0",
    "prettier": "^2.0.5"
  },
  "keywords": [
    "cdk",
    "ros",
    "code generator"
  ],
  "author": "ROS Development Team",
